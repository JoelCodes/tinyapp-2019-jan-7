<h1>Shmoo's TinyApp</h1>

<% if (user !== undefined) { %>
  <form method="POST" action="/logout">
  <span>Logged in as <b><%= user.email %></b> 👍</span>
    <label for="logout"></label>
      <input type="submit" value="Log Out" id="logout">
  </form>
  <form method="GET" action="/urls">
    <button>Your URLs</button>
  </form>
  <form method="GET" action="/urls/new">
    <button>Make Another!</button>
  </form>
<% } else { %>
  <span>
    Please <a href="/login">log in</a> or <a href="/register">register</a>.
  </span>
<% } %>
